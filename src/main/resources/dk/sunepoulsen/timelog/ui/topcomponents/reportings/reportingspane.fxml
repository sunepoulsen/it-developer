<?xml version="1.0" encoding="UTF-8"?>

<?import dk.sunepoulsen.timelog.ui.control.WeekNavigationPane?>
<?import dk.sunepoulsen.timelog.ui.topcomponents.reportings.TimeRegistrationReportingsPane?>
<?import dk.sunepoulsen.timelog.ui.topcomponents.reportings.TimeSpendReportingsPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml">
    <top>
        <WeekNavigationPane fx:id="navigationPane">
            <padding>
                <Insets bottom="10.0"/>
            </padding>
        </WeekNavigationPane>
    </top>
    <center>
        <SplitPane orientation="VERTICAL">
            <items>
                <TimeRegistrationReportingsPane fx:id="timeRegistrationsPane">
                    <padding>
                        <Insets left="10.0" top="10.0" right="10.0" bottom="10.0"/>
                    </padding>
                </TimeRegistrationReportingsPane>
                <TimeSpendReportingsPane fx:id="timeSpendPane">
                    <padding>
                        <Insets left="10.0" top="10.0" right="10.0" bottom="10.0"/>
                    </padding>
                </TimeSpendReportingsPane>
            </items>
        </SplitPane>
    </center>
</fx:root>
