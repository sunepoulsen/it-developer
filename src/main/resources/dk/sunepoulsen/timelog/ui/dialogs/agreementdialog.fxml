<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10">
    <Label text="%label.name" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

    <Label text="%label.startdate" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <DatePicker fx:id="startDateField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <Label text="%label.enddate" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <DatePicker fx:id="endDateField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>

    <Label text="%label.monday" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <TextField fx:id="mondayNormField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>

    <Label text="%label.tuesday" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <TextField fx:id="tuesdayNormField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>

    <Label text="%label.wednesday" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
    <TextField fx:id="wednesdayNormField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>

    <Label text="%label.thursday" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
    <TextField fx:id="thursdayNormField" GridPane.columnIndex="1" GridPane.rowIndex="6"/>

    <Label text="%label.friday" GridPane.columnIndex="0" GridPane.rowIndex="7"/>
    <TextField fx:id="fridayNormField" GridPane.columnIndex="1" GridPane.rowIndex="7"/>

    <Label text="%label.saturday" GridPane.columnIndex="0" GridPane.rowIndex="8"/>
    <TextField fx:id="saturdayNormField" GridPane.columnIndex="1" GridPane.rowIndex="8"/>

    <Label text="%label.sunday" GridPane.columnIndex="0" GridPane.rowIndex="9"/>
    <TextField fx:id="sundayNormField" GridPane.columnIndex="1" GridPane.rowIndex="9"/>
</fx:root>
